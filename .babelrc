{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
        },
        "useBuiltIns": "usage",
        "modules": false
      }
    ],
    "@babel/typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/proposal-class-properties", { "loose": true }],
    "@babel/proposal-object-rest-spread"
  ]
}
